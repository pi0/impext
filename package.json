{
  "name": "impext",
  "version": "0.0.1",
  "description": "",
  "repository": "pi0/impext",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "obuild",
    "dev": "obuild --stub && node --watch --import ./lib/register.mjs test/fixture/index.mts",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "automd": "^0.4.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.33.0",
    "eslint-config-unjs": "^0.5.0",
    "obuild": "^0.2.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.14.0"
}
